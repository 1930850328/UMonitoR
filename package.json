{
  "name": "hxf-monitor",
  "version": "1.0.0",
  "private": "true",
  "type": "module",
  "description": "A Lightweight SDK For Monitor Web",
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "scripts": {
    "commit": "git cz",
    "build": "rollup -c",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "publish": "changeset publish --registry=https://registry.npmjs.com/",
    "release": "pnpm build && pnpm publish",
    "lint": "eslint --fix --ext .js,.ts packages/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "commitlint": "commitlint --edit",
    "changelog": "conventional-changelog -p custom-config -i CHANGELOG.md -s"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "prettier --write"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "commitizen": "^4.3.0",
    "compare-func": "^2.0.0",
    "conventional-changelog": "^6.0.0",
    "conventional-changelog-cli": "^5.0.0",
    "conventional-changelog-custom-config": "^0.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "globals": "^15.8.0",
    "husky": "^8.0.2",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "rollup": "^4.19.1",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "rollup-plugin-uglify": "^6.0.4",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:1930850328/UMonitoR.git"
  },
  "dependencies": {
    "tslib": "^2.6.3"
  }
}
